{
  "name": "iqdb-client",
  "version": "3.0.0",
  "description": "iqdb.org api client for Node.js.",
  "scripts": {
    "test": "jest ./test/parser.test.ts ./test/util.test.ts",
    "build": "microbundle --target node -f modern,esm,cjs",
